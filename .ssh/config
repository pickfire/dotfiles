Host *.labs.overthewire.org
        SendEnv WECHALLTOKEN
        SendEnv WECHALLUSER

Host github.com git.getvase.com
        IdentityFile ~/.ssh/gh

Host hera zeus demeter
	IdentityFile ~/.ssh/vase
	User vase
	HostName %h.boxes.vase.ai
	ControlMaster auto
	ControlPersist yes
	ControlPath ~/.ssh/sock/%r@%h:%p

