Host 127.0.0.1 ::1
	CheckHostIp no
	ServerAliveInterval 15

Host *.labs.overthewire.org
	SendEnv WECHALLTOKEN
	SendEnv WECHALLUSER

Host *.onion *-tor
	ProxyCommand nc -X 5 -x 127.0.0.1:9050 %h %p
	CheckHostIP  no
	Compression  yes

