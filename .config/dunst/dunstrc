[global]
    font = Monospace 8
    allow_markup = yes
    format = "<b>%s %p</b>\n%b"
    sort = yes
    indicate_hidden = true
    show_age_threshold = 60
    geometry = "300x5-20+30"
    alignment = left
    sticky_history = yes
    idle_threshold = 120
    show_indicators = yes
    monitor = 0
    follow = mouse
    word_wrap = yes
    separator_height = 2
    padding = 10
    horizontal_padding = 10
    separator_color = frame
    startup_notification = false
    dmenu = /usr/bin/dmenu -p dunst:
    browser = /usr/bin/firefox -newtab
    icon_folders = /usr/share/icons/breeze/status/16:/usr/share/icons/breeze/devices/16


[frame]
    width = 3
    color = "#222222"

[shortcuts]
    close = ctrl+space
    close_all = ctrl+shift+space
    history = ctrl+grave
    context = ctrl+shift+period

[urgency_low]
    background = "#bbbbbb"
    foreground = "#222222"
    timeout = 10

[urgency_normal]
    background = "#005577"
    foreground = "#eeeeee"
    timeout = 15

[urgency_critical]
    background = "#ff0000"
    foreground = "#222222"
    timeout = 0


